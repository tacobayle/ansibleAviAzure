#jinja2: trim_blocks: True, lstrip_blocks: True
---
all:
  children:
  {% for item in jsonListPrivate %}
    {{ item['name'] }}:
      hosts:
    {% for addr in item['ip'] %}
        {{ addr }}:
    {% endfor %}
  {% endfor %}
  vars:
    ansible_user: "{{ ssh.username }}"
    ansible_ssh_private_key_file: "{{ ssh.privateKeyFile }}"
